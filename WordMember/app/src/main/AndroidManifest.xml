<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="supergp.wordmember">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".search.SearchList"></activity>
        <activity android:name=".search.WordDetail"></activity>
        <activity android:name=".plan.TodayPlan"></activity>
        <activity android:name=".plan.WordLearn"></activity>
        <activity android:name=".notebook.NoteList"></activity>
        <activity android:name=".notebook.WordManager"></activity>
        <activity android:name=".quiz.DayChoose"></activity>
        <activity android:name=".quiz.SingleTest"></activity>
    </application>

</manifest>